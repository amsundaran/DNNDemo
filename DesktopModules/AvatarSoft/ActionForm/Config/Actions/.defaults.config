    <Action>
        <Id>DowndloadFile</Id>
        <Title>
            <Item key="default">Send Plain Text as File Download</Item>
        </Title>
        <HelpText>
            <Item key="default">Takes the specified plain text contents and passes them to the browser for user download.</Item>
        </HelpText>
        <TypeStr>avt.ActionForm.Core.Actions.DowndloadFile, avt.ActionForm.Core</TypeStr>
        <Final>true</Final>
        <Settings>
            <Group>Message</Group>
        </Settings>

        <Parameter>
            <Id>FileName</Id>
            <Title>
                <Item key="default">File name</Item>
            </Title>
            <Type>Text</Type>
            <HelpText>
                <Item key="default">
                    Optionally provide name for the file.
                    Leave blank to use current page name. 
                    Supports <a href="http://www.dnnsharp.com/dnn/modules/my-custom-tokens" target="_blank" tabindex="-1">My Tokens</a>
                </Item>
            </HelpText>
        </Parameter>

         <Parameter>
            <Id>FileTypeId</Id>
            <Title>
                <Item key="default">File Type</Item>
            </Title>
            <Type>Select</Type>
            <Settings>
                <Items>{ "csv": { "default": "csv (text/csv)" }, "html": { "default": "html (text/html)" }, "txt": { "default": "txt (text/plain)" }, "xml": { "default": "xml (text/xml)" }, "xsl": { "default": "xsl (text/xml)" }}</Items>
                <SupportsExpressions>true</SupportsExpressions>
            </Settings>
            <HelpText>
                <Item key="default">This is the content type of the file. Supports <a href="http://www.dnnsharp.com/dnn/modules/my-custom-tokens" target="_blank" tabindex="-1">My Tokens</a></Item>
            </HelpText>
        </Parameter>

        <Parameter>
            <Id>Headers</Id>
            <Title>
                <Item key="default">Headers</Item>
            </Title>
            <Type>NameValueCollection</Type>
            <HelpText>
                <Item key="default">
                    Additional HTTP headers to pass
                </Item>
            </HelpText>
            <Settings>
                <NameColumn>Name</NameColumn>
                <ValueColumn>Value</ValueColumn>
                <ButtonName>Add</ButtonName>
            </Settings>
        </Parameter>

        <Parameter>
            <Id>FileCheck</Id>
            <Title>
                <Item key="default">Force download</Item>
            </Title>
            <Type>Boolean</Type>
            <HelpText>
                <Item key="default">
                Check if you want the browser to automatically start the file download.
                </Item>
            </HelpText>
        </Parameter>

        <Parameter>
            <Id>DowndloadFile</Id>
            <Title>
                <Item key="default">File Content</Item>
            </Title>
            <Type>Textarea</Type>
            <HelpText>
                <Item key="default">
                Specify. Supports <a href="http://www.dnnsharp.com/dnn/modules/my-custom-tokens" target="_blank" tabindex="-1">My Tokens</a>
                </Item>
            </HelpText>
        </Parameter>

    </Action>


