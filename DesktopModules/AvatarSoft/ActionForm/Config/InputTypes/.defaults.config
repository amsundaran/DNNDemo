        <Settings>
            <IsCommon>true</IsCommon>
        </Settings>

        <Parameter>
            <Id>Value</Id>
            <Title>
                <Item key="default">Initial Value</Item>
            </Title>
            <Type>Text</Type>
            <HelpText>
                <Item key="default">
                    The fields loads initially having this value.
                    Supports <a href="http://www.dnnsharp.com/dnn/modules/my-custom-tokens" target="_blank" tabindex="-1">My Tokens</a>
                    so you can pull data from various sources such as user profile.
                </Item>
            </HelpText>
        </Parameter>

        <Parameter>
            <Id>AutocompleteUrl</Id>
            <Title>
                <Item key="default">Autocomplete URL</Item>
            </Title>
            <Type>Text</Type>
            <HelpText>
                <Item key="default">
                    Provide an URL can feed autocomplete suggestions for this textbox. Use {query} where you want the typed text to go.
                </Item>
            </HelpText>
        </Parameter>
        
        <Parameter>
            <Id>Mask</Id>
            <Title>
                <Item key="default">Mask</Item>
            </Title>
            <Type>Text</Type>
            <HelpText>
                <Item key="default">
                    Optionally set an mask using the <a href="http://digitalbush.com/projects/masked-input-plugin/" target="_blank" tabindex="-1">Masked Input Plugin</a>.
                    For example, add <em>(999) 999-9999</em> for a US phone number.
                    Can contain <a href="http://www.dnnsharp.com/dnn/modules/my-custom-tokens" target="_blank" tabindex="-1">My Tokens</a>.
                </Item>
            </HelpText>
        </Parameter>

        <Parameter>
            <Id>SaveToReport</Id>
            <Title>
                <Item key="default">Save to Report</Item>
            </Title>
            <HelpText>
                <Item key="default">
                    The field will be saved in Reports.
                </Item>
            </HelpText>
            <Type>Boolean</Type>
            <DefaultValue>
                <Item key="default">true</Item>
            </DefaultValue>
        </Parameter>
        
        <Parameter>
            <Id>Readonly</Id>
            <Title>
                <Item key="default">Readonly</Item>
            </Title>
            <HelpText>
                <Item key="default">
                    The field will be Readonly.
                </Item>
            </HelpText>
            <Type>Boolean</Type>
            <DefaultValue>
                <Item key="default">false</Item>
            </DefaultValue>
        </Parameter>

        <Parameter>
            <Id>AllowTokens</Id>
            <Title>
                <Item key="default">Allow Tokens</Item>
            </Title>
            <HelpText>
                <Item key="default">
                    When this option is on, Action Form will replace tokens in input submitted by users.
                </Item>
            </HelpText>
            <Type>Boolean</Type>
        </Parameter>

