<InputTypes>

    <Type>
        <Title>Title</Title>
        <Name>static-title</Name>
        <Category>Text</Category>
        <HelpText></HelpText>
        <TypeStr>avt.ActionForm.Core.Input.TextBasedField,avt.ActionForm.Core</TypeStr>

        <Settings>
            <HasInputValidation>false</HasInputValidation>
            <HasValue>true</HasValue>
        </Settings>

        <Parameter>
            <Id>Size</Id>
            <Title>
                <Item key="default">Size</Item>
            </Title>
            <Type>Select</Type>
            <Settings>
                <Items>{ "h1": { "default": "H1 (biggest)" }, "h2": { "default": "H2" }, "h3": { "default": "H3" }, "h4": { "default": "H4" }, "h5": { "default": "H5" }, "h6": { "default": "H6" }, "h7": { "default": "H7 (smallest)" } }</Items>
            </Settings>
            <DefaultValue>
                <Item key="default">h2</Item>
            </DefaultValue>
        </Parameter>

        <Parameter>
            <Id>ShowLine</Id>
            <Title>
                <Item key="default">Horizontal Line</Item>
            </Title>
            <Type>Boolean</Type>
            <DefaultValue>
                <Item key="default">true</Item>
            </DefaultValue>
        </Parameter>

        <Parameter>
            <Id>SaveToReport</Id>
            <Title>
                <Item key="default">Save to Report</Item>
            </Title>
            <HelpText>
                <Item key="default">
                    The field will be saved in Reports.
                </Item>
            </HelpText>
            <Type>Boolean</Type>
            <DefaultValue>
                <Item key="default">true</Item>
            </DefaultValue>
        </Parameter>
    </Type>

    <Type>
        <Title>Static Text</Title>
        <Name>static-text</Name>
